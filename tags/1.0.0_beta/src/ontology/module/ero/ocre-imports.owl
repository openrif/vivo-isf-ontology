    <owl:Class rdf:about="http://purl.org/net/OCRe/research.owl#Qualitative_human_study">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Qualitative human study</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/ERO_0000015"/>
        <obo:IAO_0000115 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A qualitative study is an individual-human study whose primary mode of inquiry is qualitative.</obo:IAO_0000115>
